# #  find out odd and even numbers using filters functions
# num=[1,2,3,4,5,6,7,8,9,0,22,34,44]

# odd=filter(lambda i:i%2!=0 ,num)
# even=filter(lambda i:i%2==0,num)
# print(list(odd))
# print(list(even))


LIST=[1,2,3,4,5,6,]
for i in LIST:
    
    if i%2!=0:
        print(i)
    else:

        print('false')


else:
    LIST.append(i)
    i%2==0
    print(i,'odd')
print(LIST)